    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JaimecCoder\\ArchitectureLibrary\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Eduardo",
            "email": "centurionjaime@gmail.com"
        }
    ],
    "extra": {
